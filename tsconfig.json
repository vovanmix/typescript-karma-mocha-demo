        "jsx": "react",
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": true,
        "sourceMap": false,
        "strictNullChecks": true,
        "preserveConstEnums": true,
        "removeComments": true,
        "outDir": "./.ts.build/"
